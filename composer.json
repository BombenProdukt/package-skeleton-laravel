{
    "name": "preemstudio/:package_slug",
    "description": ":package_description",
    "keywords": [
        "PreemStudio",
        "laravel",
        ":package_slug"
    ],
    "homepage": "https://github.com/preemstudio/:package_slug",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@preem.studio",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.1",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.2",
        "graham-campbell/analyzer": "^3.1",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.2",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ray": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "PreemStudio\\Skeleton\\": "src",
            "PreemStudio\\Skeleton\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PreemStudio\\Skeleton\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "PreemStudio\\Skeleton\\SkeletonServiceProvider"
            ],
            "aliases": {
                "Skeleton": "PreemStudio\\Skeleton\\Facades\\Skeleton"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
